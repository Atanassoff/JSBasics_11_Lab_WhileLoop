function demo(input){
    let people = Number(input[0]);
    let entrance = Number(input[1]);
    let chaiseLongue = Number(input[2]);
    let umbrella = Number(input[3]);
    let total = people * entrance + Math.ceil(people * 0.75) * chaiseLongue + Math.ceil(people / 2) * umbrella; 
    console.log(`${total.toFixed(2)} lv.`);
}
